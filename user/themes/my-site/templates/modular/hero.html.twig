<section class="hero home-section fp-auto-height-responsive" style="{{ backgroundImage }}">
    <div class="moving-lines">
        <div class="circle circle-1"></div>
        <div class="circle circle-2"></div>
        <div class="circle circle-3"></div>
        <div class="circle circle-4"></div>
        <div class="line line-1"></div>
        <div class="line line-2"></div>
        <div class="line line-3"></div>
        <div class="line line-4"></div>
        <div class="line line-5"></div>
        <div class="line line-6"></div>
        <div class="square square-1"></div>
        <div class="square square-2"></div>
        <div class="square square-3"></div>
    </div>
    <div class="profile-pic"></div>
    {# <div class="bg-circle"></div> #}
    <div class="bottom-title">{{page.header.title}}</div>
    <div class="hero__content">
        <div class="hero__content--inner">
            <div class="vert-text">{{page.header.vert_text}}</div>
            {# <div class="h1">Ed Franks</div> #}
            {{ page.content }}
        </div>
    </div>
    <a class="scroll" href="#vaujany" data-scroll>scroll</a>
</section>